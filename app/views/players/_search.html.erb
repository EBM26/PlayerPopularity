<%= form_for @people, html:{:class => "form"} do |f| %>
  <%= f.text_field :hi, class: 'inputbox'%>
<% end %>

<script>
  searchable = [];  // this will hold our list of players

  // Get list of searchable players from the API
  $.getJSON("api/players/list", function(data) {
    
    $.each( data, function( key, val ) {
      var player = {};
      player[val.id] = val.name;
      searchable.push(player);
    });

    console.log(searchable);
  });

</script>


